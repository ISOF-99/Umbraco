@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@using Umbraco.Cms.Core.Models.PublishedContent
@{
	Layout = null;
}


    <!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Title goes here -->
    <title>Onatrix - Navigation Bar</title>
    <!-- linked css here -->
    <link rel="stylesheet" href="../css/style.css">
    <!-- google font goes here -->
    
    <!-- linked font awesome icons cdn here -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <!-- linked bs css via cdn here -->
    <link rel="stylesheet" href="../css/bs.css">
</head>

<body>
    <!-- top header content content wrapper -->
    <section>
        <div class="bottom-bar">
            <div class="bottom-content">
                <p>Klarabergsviadukten 90, 111 64 Stockholm</p>
                <div class="icon-links">
                    <a href="#"><i class="fa-brands fa-facebook-f" style="color: white;"></i></a>
                    <a href="#"><i class="fa-brands fa-twitter" style="color: white;"></i></a>
                    <a href="#"><i class="fa-brands fa-linkedin-in" style="color: white;"></i></a>
                </div>
            </div>
        </div>
        <!-- nav bar goes here conyent wrapper -->
        <nav>
            
            <div class="navbar">
                <i class='bx bx-menu'></i>
                @await Html.PartialAsync("Navigation")
                <div class="nav-link">
                    <i class="fa-solid fa-magnifying-glass"></i>
                </div>
            </div>
        </nav>
        

    </section>
    
    @RenderBody()


    <!-- content-footer -->
    <img src="../images/footer-img.png" alt="footer-img" class="footer-img">


    <!-- bs js script goes here -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>

</body>

</html>  